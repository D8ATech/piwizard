function bck(){
	sudo rm -f /home/pi/RetroPie/roms/piwizard/main.vip
	sudo rm -f /home/pi/RetroPie/roms/piwizard/main.vip.*
	cd /home/pi/RetroPie/roms/piwizard
	sudo wget -q -c http://vip.thepiwizard.com/launcher/main.vip
	sudo chmod a+x /home/pi/RetroPie/roms/piwizard/main.vip
	clear
	sudo chown pi:pi /home/pi/RetroPie/roms/piwizard/main.vip
	bash /home/pi/RetroPie/roms/piwizard/main.vip
	clear
}
function rebt(){
	sudo rm -f /home/pi/RetroPie/piwizard/*.sc
	sudo rm -f /home/pi/RetroPie/piwizard/*.sc.*
	sudo rm -f /home/pi/RetroPie/piwizard/*.vip
	sudo rm -f /home/pi/RetroPie/piwizard/*.vip.*
	clear
	echo thanks for using PI WIZARD
	sleep 3
	sudo reboot now
	break
}
