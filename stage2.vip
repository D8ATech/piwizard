#!/bin/bash
INPUT=/tmp/menu.sh.$$
OUTPUT=/tmp/output.sh.$$
trap "rm $OUTPUT; rm $INPUT; exit" SIGHUP SIGINT SIGTERM
function display_output(){
	local h=${1-82}
	local w=${2-82}
	local t=${3-Output}
	dialog --backtitle  "Pro Version Romset Installation" --title "${t}" --msgbox "$(<$OUTPUT)" ${h} ${w}
}
function atari2600(){
	clear
	mkdir /home/pi/RetroPie/roms/atari2600
	cd /home/pi/RetroPie/roms/atari2600
	sudo rm -f /home/pi/RetroPie/roms/atari2600/atari2600.zip.*
	sudo rm -f /home/pi/RetroPie/roms/atari2600/atari2600.zip
	clear
	echo Currently Downloading Atari 2600 Rom Pack
	wget -q -c --show-progress --trust-server-names http://roms.thepiwizard.com/pro/atari2600.zip
	unzip -o /home/pi/RetroPie/roms/nes/nes.zip -d /home/pi/RetroPie/roms/atari2600
	sudo rm -f /home/pi/RetroPie/roms/atari2600/atari2600.zip
	clear
	echo Atari 2600 Rom Pack Installed
	sleep 3
	###sudo reboot now
	cd /home/pi
}
